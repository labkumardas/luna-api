<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Service</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Service List</li>
            </ol>
          </div>
        </div>
      </div>
      <!-- /.container-fluid -->
    </section>
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
              <div class="card">
                  <div class="card-header">
                    <h3 class="card-title">Service List</h3>
                  </div>
                  <!-- /.card-header -->
                  <div class="card-body">
                    <table id="example1" class="table table-bordered ">
                      <thead>
                        <tr>
                          <th>#  </th>
                          <th>Title  </th>
                          <th>Category  </th>
                          <th>Sub Category  </th>
                          <th>Image  </th>
                          <th>Type  </th>
                          <th>Description  </th>
                        
                          <th>Action</th>
                        </tr>
                        </thead>
                        <tbody>
                         
                          <% response.forEach(function(data, index){ %>
                             <% const desc=data.description.substring(0,90) ; %>
                            <tr>
                              <td> <%= index +1 %> </td>
                              <td> <%= data.title %> </td>
                              <td> <%= data.Category.name %></td>
                              <td> <%= data.Subcategory.name %></td>
                              <td> <img src="<%= data.image %>"  style="width: 125px;height: 45px;"></td>
                              <td> <%= data.property_type  %></td>
                              <td> <%= desc.replace(/[^a-zA-Z ]/g, "");  %>......</td>
                              <td><a href ="" >  Edit </a> | <a href ="" >  Delete </a> </td>
                            </tr>
                    
                    <% } ) %>
                   
                        
                      
                      </tbody>
                      
                    </table>
                  </div>
                  <!-- /.card-body -->
                </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
  </div>
    <!-- Page specific script -->
  
  </div>
  