<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>User</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">User List</li>
            </ol>
          </div>
        </div>
      </div>
      <!-- /.container-fluid -->
    </section>
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
              <div class="card">
                  <div class="card-header">
                    <h3 class="card-title">User List</h3>
                  </div>
                  <!-- /.card-header -->
                  <div class="card-body">
                    <table id="example1" class="table table-bordered ">
                      <thead>
                        <tr>
                          <th>#</th>
                          <th>Name</th>
                          <th>Email</th>
                          <th>Phone</th>
                          <th>State</th>
                          <th>City</th>
                          <th>Pin</th>
                          <th>Photo</th>
                          <th>CreatedAt</th>
                          <th>Action</th>
                        </tr>
                        </thead>
                        <tbody>
                         
                          <% response.forEach(function(data, index){ %>
                          
                            <tr>
                              <td> <%= index +1 %> </td>
                              <td> <%= data.name %> </td>
                              <td> <%= data.email %></td>
                              <td> <%= data.phone %></td>
                              
                              <td> <%= data.state %></td>
                              <td> <%= data.city %></td>
                              <td> <%= data.pin %></td>
                              <td> <img src="<%= data.photo %>"  style="width: 125px;height: 45px;"></td>
                              <td> <%= data.createdAt.toLocaleString()  %></td>
                             
                              <td><a href ="" >  Edit </a> | <a href ="" >  Delete </a> </td>
                            </tr>
                    
                    <% } ) %>
                   
                        
                      
                      </tbody>
                      
                    </table>
                  </div>
                  <!-- /.card-body -->
                </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
  </div>
    <!-- Page specific script -->
  
  </div>
  